{
  "name": "agi-causal-knowledge-base-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "devEngines": {
    "packageManager": {
      "name": "npm",
      "onFail": "warn"
    }
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --project unit",
    "test:browser": "vitest --project browser",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate": "json2ts src/modules/causal-knowledge/graphical-causal-knowledge-base-v1.schema.json src/modules/causal-knowledge/graphicalCausalKnowledgeBase.d.ts --bannerComment \"$(cat ./json2ts.banner.txt)\"",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "license-headers:check": "license-check-and-add check -f check-license-header.json",
    "license-headers:fix": "license-check-and-add add -f check-license-header.json -r 2026",
    "lint:check": "run-s license-headers:check eslint:check",
    "lint:fix": "run-s license-headers:fix eslint:fix",
    "format:check": "prettier --check src/",
    "format:fix": "prettier --write src/",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "generate-attributions": "node ./generate-attributions.ts"
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "1.20.1",
    "@vueform/multiselect": "2.6.11",
    "@vueuse/core": "14.2.0",
    "ajv": "8.17.1",
    "bulma": "1.0.4",
    "file-saver": "2.0.5",
    "pinia": "3.0.4",
    "vue": "3.5.27",
    "vue-router": "4.6.4",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "1.58.0",
    "@tsconfig/node24": "24.0.4",
    "@types/file-saver": "2.0.7",
    "@types/jsdom": "27.0.0",
    "@types/license-checker": "25.0.6",
    "@types/node": "24.10.9",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitejs/plugin-vue-jsx": "5.1.3",
    "@vitest/browser": "4.0.18",
    "@vitest/eslint-plugin": "1.6.6",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-markdown-ext": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-hint": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-image": "2.0.0-rc.121",
    "@vuepress/theme-default": "2.0.0-rc.121",
    "eslint": "9.39.2",
    "eslint-plugin-playwright": "2.5.1",
    "eslint-plugin-vue": "10.7.0",
    "jiti": "2.6.1",
    "jsdom": "27.4.0",
    "json-schema-to-typescript": "15.0.4",
    "license-check-and-add": "4.0.5",
    "license-checker": "25.0.1",
    "msw": "2.12.7",
    "npm-run-all2": "8.0.4",
    "playwright": "1.58.0",
    "prettier": "3.8.1",
    "sass-embedded": "1.97.3",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-static-copy": "3.2.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vitest": "4.0.18",
    "vitest-browser-vue": "2.0.2",
    "vue-tsc": "3.2.4",
    "vuepress": "2.0.0-rc.26"
  }
}
