{
  "name": "agi-causal-knowledge-base-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --project unit",
    "test:browser": "vitest --project browser",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate": "json2ts src/assets/graphical-causal-knowledge-base-v1.schema.json src/model/graphicalCausalKnowledgeBase.d.ts --bannerComment \"$(cat ./json2ts.banner.txt)\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check src/",
    "format:fix": "prettier --write src/"
  },
  "dependencies": {
    "@vueform/multiselect": "^2.6.11",
    "@vueuse/core": "^13.0.0",
    "ajv": "^8.17.1",
    "bulma": "^1.0.3",
    "file-saver": "^2.0.5",
    "pinia": "^3.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@tsconfig/node22": "22.0.2",
    "@types/file-saver": "2.0.7",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.18.8",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vitest/browser": "4.0.0-beta.11",
    "@vitest/eslint-plugin": "1.3.12",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.35.0",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-vue": "10.4.0",
    "jiti": "2.5.1",
    "jsdom": "27.0.0",
    "json-schema-to-typescript": "15.0.4",
    "msw": "2.11.3",
    "npm-run-all2": "8.0.4",
    "playwright": "1.55.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-static-copy": "3.1.2",
    "vite-plugin-vue-devtools": "8.0.2",
    "vitest": "4.0.0-beta.11",
    "vitest-browser-vue": "1.1.0",
    "vue-tsc": "3.0.7"
  }
}
