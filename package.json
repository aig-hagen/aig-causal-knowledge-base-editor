{
  "name": "agi-causal-knowledge-base-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "devEngines": {
    "packageManager": {
      "name": "npm",
      "onFail": "warn"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --project unit",
    "test:browser": "vitest --project browser",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate": "json2ts src/assets/graphical-causal-knowledge-base-v1.schema.json src/model/graphicalCausalKnowledgeBase.d.ts --bannerComment \"$(cat ./json2ts.banner.txt)\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check src/",
    "format:fix": "prettier --write src/",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@vueform/multiselect": "2.6.11",
    "@vueuse/core": "14.1.0",
    "ajv": "8.17.1",
    "bulma": "1.0.4",
    "file-saver": "2.0.5",
    "pinia": "3.0.4",
    "vue": "3.5.25",
    "vue-router": "4.6.4",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "@tsconfig/node22": "22.0.5",
    "@types/file-saver": "2.0.7",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.3",
    "@vitejs/plugin-vue": "6.0.2",
    "@vitejs/plugin-vue-jsx": "5.1.2",
    "@vitest/browser": "4.0.15",
    "@vitest/eslint-plugin": "1.5.2",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-markdown-ext": "2.0.0-rc.120",
    "@vuepress/plugin-markdown-hint": "2.0.0-rc.120",
    "@vuepress/plugin-markdown-image": "2.0.0-rc.120",
    "@vuepress/theme-default": "2.0.0-rc.120",
    "eslint": "9.39.1",
    "eslint-plugin-playwright": "2.4.0",
    "eslint-plugin-vue": "10.6.2",
    "jiti": "2.6.1",
    "jsdom": "27.3.0",
    "json-schema-to-typescript": "15.0.4",
    "msw": "2.12.4",
    "npm-run-all2": "8.0.4",
    "playwright": "1.57.0",
    "prettier": "3.7.4",
    "sass-embedded": "1.96.0",
    "typescript": "5.9.3",
    "vite": "7.2.7",
    "vite-plugin-static-copy": "3.1.4",
    "vite-plugin-vue-devtools": "8.0.5",
    "vitest": "4.0.15",
    "vitest-browser-vue": "2.0.1",
    "vue-tsc": "3.1.8",
    "vuepress": "2.0.0-rc.26"
  }
}
