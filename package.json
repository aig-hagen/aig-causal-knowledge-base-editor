{
  "name": "agi-causal-knowledge-base-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate": "json2ts src/assets/graphical-causal-knowledge-base-v1.schema.json src/model/graphicalCausalKnowledgeBase.d.ts --bannerComment \"$(cat ./json2ts.banner.txt)\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check src/",
    "format:fix": "prettier --write src/"
  },
  "dependencies": {
    "@vueform/multiselect": "^2.6.11",
    "@vueuse/core": "^13.0.0",
    "ajv": "^8.17.1",
    "bulma": "^1.0.3",
    "file-saver": "^2.0.5",
    "monaco-editor": "^0.52.2",
    "pinia": "^3.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "1.53.2",
    "@tsconfig/node22": "22.0.2",
    "@types/file-saver": "2.0.7",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.16.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "4.2.0",
    "@vitest/eslint-plugin": "1.3.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.30.1",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-vue": "10.3.0",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "json-schema-to-typescript": "15.0.4",
    "msw": "2.10.2",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-static-copy": "3.1.0",
    "vite-plugin-vue-devtools": "7.7.7",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.1"
  },
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "onfail": "warn"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw",
      "vue-demi"
    ]
  }
}
